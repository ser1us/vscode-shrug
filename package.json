{
  "name": "shruggoff",
  "displayName": "Shrug",
  "description": "Add a shrug ascii emoji at your current cursor position :)",
  "publisher": "Christoph Lipp",
  "icon": "icons/xtr-shrug-1.png",
  "repository": "https://github.com/ser1us/vscode-shrug",
  "bugs": {
    "url": "https://github.com/ser1us/vscode-shrug/issues"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.66.0"
  },
  "scripts": {
    "deploy": "vsce publish --yarn"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "",
        "path": "./snippets/snippets.code-snippets"
      }
    ],
    "commands": [
      {
        "title": "Insert shrug at current position",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
          "langId": "",
          "name": "shrug"
        }
      }
    ],
    "keybindings": [
      {
        "key": "Ctrl+U Ctrl+U",
        "title": "Insert shrug at current position",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
          "langId": "",
          "name": "shrug"
        }
      }
    ]
  },
  "dependencies": {
    "vsce": "^2.7.0"
  }
}
